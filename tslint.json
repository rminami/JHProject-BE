{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "trailing-comma": [true, "never"],
    "quotemark": [true, "single", "avoid-escape"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "rulesDirectory": []
}
